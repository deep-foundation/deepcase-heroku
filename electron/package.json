    "electron-updater": "~4.3.9",
    "electron-window-state": "~5.0.3",
    "express": "^4.17.1",
    "fix-path": "^4.0.0",
    "fkill": "^7.2.1",
    "socket.io": "^4.2.0"
  },
  "devDependencies": {
    "electron": "~13.1.9",
    "electron-builder": "~22.11.7",
    "electron-rebuild": "~2.3.5",
    "typescript": "~4.3.5"
